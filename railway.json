{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "builder": "dockerfile",
    "dockerfilePath": "Dockerfile.svelte"
  },
  "deploy": {
    "startCommand": "node -r dotenv/config server/app.js",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100,
    "restartPolicyType": "on_failure"
  }
}
